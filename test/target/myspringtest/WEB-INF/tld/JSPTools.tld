<?xml version="1.0" encoding="UTF-8"?>

<taglib xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee web-jsptaglibrary_2_0.xsd"
	version="2.0">

	<description>JSTL 1.1 core library</description>
	<display-name>JSTL core</display-name>
	<tlib-version>1.1</tlib-version>
	<short-name>JSPTools</short-name>

	<function>
       <description>格式化日期</description>
       <name>dateFormat</name> 
       <function-class>com.tld.JSPTools</function-class>
       <function-signature>java.lang.String dateFormat(java.lang.Object,java.lang.String)</function-signature>
       <example>${my:dateFormat(obj1, str2)}</example>
    </function>
    
    <function>
       <description>去空格</description>
       <name>trim</name> 
       <function-class>com.tld.JSPTools</function-class>
       <function-signature>java.lang.String trim(java.lang.Object)</function-signature>
       <example>${my:trim(obj1)}</example>
    </function>
        <function>
       <description>在指定字符之前插入字符串</description>
       <name>insert</name> 
       <function-class>com.tld.JSPTools</function-class>
       <function-signature>java.lang.String cutstring(java.lang.String,java.lang.String,java.lang.String)</function-signature>
       <example>${my:insert(obj1,obj2,obj3)}</example>
    </function>
    
    <function>
       <description>四舍五入</description>
       <name>round</name> 
       <function-class>com.tld.JSPTools</function-class>
       <function-signature>java.lang.String round(java.lang.Object,java.lang.Integer)</function-signature>
       <example>${my:round(obj1,int1)}</example>
    </function>
    
    
    
    <function>
       <description>字符大于多少位后省略</description>
       <name>strLength</name> 
       <function-class>com.tld.JSPTools</function-class>
       <function-signature>java.lang.String strLength(java.lang.Object,java.lang.Integer)</function-signature>
       <example>${my:strLength(obj1,int1)}</example>
    </function>
</taglib>